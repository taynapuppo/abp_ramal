@using SistemaRamais.Ramais;
@using System.Globalization
@inject IHtmlLocalizer<SistemaRamaisResource> L
@model EditModalModel
@{
    Layout = null;
}

<form data-ajaxForm="true" asp-page="/Ramais/EditModal" autocomplete="off">
    <abp-modal id="RamalEditModal">
