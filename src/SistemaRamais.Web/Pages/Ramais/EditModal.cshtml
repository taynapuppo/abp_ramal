        <abp-modal-body>
@*//<suite-custom-code-block-1>*@
@*//</suite-custom-code-block-1>*@

            <abp-input asp-for="Id" />

                
                    
            <abp-input asp-for="Ramal.ConcurrencyStamp" hidden="true" suppress-label="true"/>
            

